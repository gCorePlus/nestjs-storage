{
  "name": "@gcoreplus/nestjs-storage",
  "version": "0.0.4",
  "description": "Provides an injectable storage (S3, GS) client to provide storage access from nestjs modules",
  "author": "Rennan Stefan Boni",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/",
    "README.MD",
    "LICENCE.MD"
  ],
  "homepage": "https://rennanboni.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gCorePlus/nestjs-storage.git"
  },
  "contributors": [
    {
      "name": "Rennan Stefan Boni",
      "email": "rennanboni@gmail.com"
    }
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "format": "prettier --write \"lib/**/*.ts\"",
    "publish:npm": "npm publish --access public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:clean": "rm -rf coverage",
    "test:codecov": "codecov",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "keywords": [
    "nestjs",
    "node.js",
    "s3",
    "aws",
    "google",
    "storage"
  ],
  "license": "ISC",
  "peerDependencies": {
    "@nestjs/common": "^7.1.3",
    "@nestjs/core": "^7.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@nestjs/common": "^7.1.3",
    "@nestjs/core": "^7.1.3",
    "@nestjs/testing": "^7.1.3",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "codecov": "^3.7.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "ts-jest": "^26.1.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.0.1",
    "aws-sdk": "^2.694.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
